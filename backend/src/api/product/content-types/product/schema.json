{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "rating": {
      "required": true,
      "min": 1,
      "max": 10,
      "default": 1,
      "type": "integer"
    },
    "desc": {
      "type": "text",
      "required": true
    },
    "img": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "isLiked": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "product_id": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "category": {
      "type": "string",
      "required": true
    }
  }
}
